<h1>Welcome <%= current_user.name %>!</h1>
<br>
<%= button_to "Discover Movies", discover_path, method: :get %>
<br>
<section id="friends-<%=current_user.id%>">
  <h3>Friends</h3>
  <%= form_with url: friends_path, local: true do |f| %>
    <%= f.label "Friend's Email: " %>
    <%= f.text_field :email %>
    <%= f.submit "Add Friend" %>
  <% end %>
</section>
